<form action="/users/signin" method="post">
  <div>
    <label>Username:
      <input name="username" type="text" value=<%= params[:username] %>>
    </label>
  </div>
  <div>
    <label>Password:
      <input name="password" type="password">
    </label>
  </div>
  <button type="submit">Sign In</button>
</form>